<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net6.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWPF>true</UseWPF>
    <StartupObject>MainNetworkingProject.App</StartupObject>
	 <GenerateRuntimeConfigurationFiles>True</GenerateRuntimeConfigurationFiles>
  </PropertyGroup>


  <ItemGroup>
	 <Compile Include=".model\basics\AsyncObservableCollection.cs" />
	 <Compile Include=".model\basics\first-try\ExplorerClient.cs" />
	 <Compile Include=".model\basics\first-try\ExplorerService.cs" />
	 <Compile Include=".model\basics\first-try\ServiceUser.cs" />
	 <Compile Include=".model\basics\second-try\model-related\PacketBuilder.cs" />
	 <Compile Include=".model\basics\second-try\model-related\PacketReader.cs" />
	 <Compile Include=".model\basics\second-try\model-related\ReversedClient.cs" />
	 <Compile Include=".model\basics\second-try\model-related\ReversedService.cs" />
	 <Compile Include=".model\basics\second-try\model-related\UserModel.cs" />
	 <Compile Include=".model\basics\second-try\viewmodel-related\RelayCommand.cs" />
	 <Compile Include=".model\basics\second-try\viewmodel-related\ServiceHub.cs" />
	 <Compile Include=".view-model\first-try\ClientWindow\ClientWindowViewModelHandler.cs" />
	 <Compile Include=".view-model\MainWindow\MainWindowViewModelHandler.cs" />
	 <Compile Include=".view-model\second-try\ReversedClientWindow\ReversedClientWindowViewModel.cs" />
	 <Compile Include=".view-model\first-try\ServiceWindow\ServiceWindowViewModelHandler.cs" />
	 <Compile Include=".view-model\MainWindow\MainWindowViewModelState.cs" />
	 <Compile Include=".view-model\first-try\ClientWindow\ClientWindowViewModel.cs" />
	 <Compile Include=".view-model\MainWindow\MainWindowViewModel.cs" />
	 <Compile Include=".view-model\first-try\ServiceWindow\ServiceWindowViewModel.cs" />
	 <Compile Include=".view\App.xaml.cs" />
	 <Compile Include=".view\ClientWindow.xaml.cs" />
	 <Compile Include=".view\MainWindow.xaml.cs" />
	 <Compile Include=".view\ServiceWindow.xaml.cs" />
	 <Page Include=".view\ClientWindow.xaml">
	   <Generator>MSBuild:Compile</Generator>
	 </Page>
    <Page Include=".view\MainWindow.xaml" />
    <Page Include=".view\ServiceWindow.xaml">
      <Generator>MSBuild:Compile</Generator>
    </Page>
    <ApplicationDefinition Include=".view\App.xaml">
		<XamlRuntime>$(DefaultXamlRuntime)</XamlRuntime>
		<SubType>Designer</SubType>
      <Generator>MSBuild:Compile</Generator>
    </ApplicationDefinition>
  </ItemGroup>


  <ItemGroup>
    <PackageReference Include="Prism.Core" Version="8.1.97" />
  </ItemGroup>

</Project>
